import json

FILE = "practice.json"

def load_data():
    try:
        with open(FILE, "r") as f:
            return json.load(f)
    except FileNotFoundError:
        return []

def save_data(data):
    with open(FILE, "w") as f:
        json.dump(data, f, indent=4)

name = input("Enter name: ")
age = input("Enter age: ")

records = load_data()
records.append({"name": name, "age": age})
save_data(records)

print("Data saved successfully")
